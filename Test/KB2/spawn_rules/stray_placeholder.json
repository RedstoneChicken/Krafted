        },
        "minecraft:weight": {
          "default": 1750
        },
        "minecraft:herd": {
          "min_size": 2,
          "max_size": 4
        },
        "minecraft:height_filter": {
          "min": -63,
          "max": 55
        },
        "minecraft:biome_filter": {
          "test": "has_biome_tag", "operator": "==", "value": "frozen"
        }
      }
    ]
  }
}